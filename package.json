{
  "name": "customer-portal",
  "version": "11.68.1",
  "scripts": {
    "\n# LOCAL DEV SCRIPTS:": "",
    "start": "npm run transloco:extract-scoped-libs && npm run i18n:cache-busting && ng serve -c dev",
    "start:local-prod": "cd dist/browser && http-server -c-1 .",
    "start:ssr": "node dist/server",
    "start:ssr:debug": "set DEBUG=express:* & node dist/server",
    "\n# BUILD SCRIPTS:": "",
    "build": "npm run transloco:extract-scoped-libs && npm run transloco:optimize && npm run i18n:cache-busting && node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build",
    "build:local-prod": "npm run transloco:extract-scoped-libs && npm run transloco:optimize && npm run i18n:cache-busting && node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build --prod --configuration local-prod && gulp local-prod --origin https://travaux.dev-homegenius.fr",
    "build:browser:develop": "npm run transloco:extract-scoped-libs && npm run transloco:optimize && npm run i18n:cache-busting && node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build --prod --configuration prod-develop && gulp prod --origin https://travaux.dev-homegenius.fr",
    "build:browser:preprod": "npm run transloco:extract-scoped-libs && npm run transloco:optimize && npm run i18n:cache-busting && node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build --prod --configuration prod-preprod && gulp prod --origin https://travaux.preprod-homegenius.fr",
    "build:browser:master": "npm run transloco:extract-scoped-libs && npm run transloco:optimize && npm run i18n:cache-busting && node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build --prod --configuration prod-master && gulp prod --origin https://travaux.leroymerlin.fr",
    "build:server:develop": "ng run customer-portal:server:prod-develop && gulp ssr --origin https://travaux.dev-homegenius.fr",
    "build:server:master": "ng run customer-portal:server:prod-master && gulp ssr --origin https://travaux.leroymerlin.fr",
    "build:client-and-server-bundles:develop": "concurrently \"npm run build:browser:develop\" \"npm run build:server:develop\"",
    "build:client-and-server-bundles:master": "concurrently \"npm run build:browser:master\" \"npm run build:server:master\"",
    "build:ssr:develop": "concurrently \"npm run build:client-and-server-bundles:develop\" \"npm run compile:server\"",
    "build:ssr:master": "concurrently \"npm run build:client-and-server-bundles:master\" \"npm run compile:server\"",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "\n# TEST SCRIPTS:": "",
    "test": "ng test customer-portal",
    "test:ci": "npm run transloco:extract-scoped-libs && npm run i18n:cache-busting && ng test customer-portal --karmaConfig=./src/karma.conf.ci.js",
    "e2e": "ng e2e",
    "\n# LINT SCRIPTS:": "",
    "lint": "concurrently \"npm run stylelint\" \"npm run tslint\"",
    "stylelint": "stylelint \"**/*.scss\"",
    "tslint": "ng lint",
    "\n# UTILS SCRIPTS:": "",
    "update:dependencies": "ng update @angular/core @angular/cli @angular/cdk document-register-element @datorama/akita-cli @nguniversal/common @nguniversal/express-engine @nguniversal/module-map-ngfactory-loader @angular-builders/custom-webpack @angular/material @angular/platform-server @angular/platform-browser @datorama/akita @angular/pwa @types/googlemaps @types/jasmine @types/jasminewd2 @types/lodash karma jasmine-core ngx-logger ngx-toastr rxjs-tslint-rules stylelint stylelint-config-standard stylelint-scss",
    "cz": "git add . && git-cz && git push-upstream",
    "akita": "akita",
    "semver": "npx semantic-release",
    "analyze:local-prod": "webpack-bundle-analyzer dist/browser/stats.json",
    "ng": "ng",
    "clean": "rimraf coverage dist",
    "clean:browser:assets": "rimraf dist/browser/assets",
    "circular-dependencies-check": "npx madge ./src/ --circular --extensions ts,js --image ./dist/dependencies-graph.svg --warning",
    "transloco:extract-scoped-libs": "transloco-scoped-libs",
    "transloco:optimize": "transloco-optimize dist/customer-portal/assets/i18n",
    "i18n:cache-busting": "node ./scripts/node/i18n-cache-busting.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/cdk": "~8.2.3",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/elements": "~8.2.14",
    "@angular/forms": "~8.2.14",
