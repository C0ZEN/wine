    "madge": "~3.6.0",
    "mock-browser": "~0.92.14",
    "moment-locales-webpack-plugin": "~1.1.0",
    "moment-timezone-data-webpack-plugin": "~1.1.0",
    "ng-bullet": "~1.0.3",
    "protractor": "~5.4.2",
    "rimraf": "~2.6.3",
    "rxjs-tslint-rules": "~4.28.0",
    "schema-dts": "~0.4.5",
    "semantic-release": "~15.13.21",
    "stylelint": "~12.0.1",
    "stylelint-config-standard": "~19.0.0",
    "stylelint-scss": "~3.13.0",
    "ts-loader": "~5.2.0",
    "ts-node": "~8.0.3",
    "tslint": "~5.17.0",
    "tslint-etc": "~1.5.3",
    "typescript": "~3.5.3",
    "webpack": "~4.41.0",
