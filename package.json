    "@ngx-lite/json-ld": "~0.6.2",
    "@ngx-pwa/local-storage": "~8.2.1",
    "@thisissoon/angular-inviewport": "~4.2.0",
    "animate.css": "~3.7.2",
    "browser-detect": "~0.2.28",
    "core-js": "~2.6.10",
    "express": "~4.15.2",
    "fg-loadcss": "~2.1.0",
    "hammerjs": "~2.0.8",
    "intersection-observer": "~0.7.0",
    "localforage": "~1.7.3",
    "lodash": "~4.17.15",
    "messageformat": "~2.3.0",
    "moment": "~2.24.0",
    "moment-timezone": "~0.5.27",
    "rxjs": "~6.5.3",
    "tslib": "~1.9.3",
    "zone.js": "~0.9.1"
