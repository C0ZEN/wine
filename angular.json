{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "customer-portal": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {
        "@schematics/angular:component": {
          "styleext": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "customWebpackConfig": {
              "path": "./scripts/webpack/custom-webpack-build.config.js",
              "replaceDuplicatePlugins": false
            },
            "outputPath": "dist/browser",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/assets",
              "src/manifest.json",
              "src/robots.txt",
              "src/favicon.ico"
            ],
            "styles": [
              "src/assets/styles/styles.scss",
              {
                "input": "node_modules/ng-homegenius-portal/assets/fonts/montserrat/Montserrat.css",
                "lazy": true,
                "bundleName": "lazy-styles"
              },
              {
                "input": "node_modules/animate.css/animate.min.css",
                "lazy": true,
                "bundleName": "lazy-styles"
              }
            ],
            "es5BrowserSupport": true
          },
          "configurations": {
            "dev": {
              "progress": false,
              "verbose": false,
              "optimization": false
            },
            "local-prod": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/browser/environment-browser.prod-master.ts"
                },
                {
                  "replace": "src/manifest.json",
                  "with": "src/manifests/manifest.prod-master.json"
                },
                {
                  "replace": "src/robots.txt",
                  "with": "src/robots/robots.prod-master.txt"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ],
              "serviceWorker": true,
              "progress": true,
              "statsJson": true,
              "extractLicenses": true,
              "verbose": false
            },
            "prod-develop": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/browser/environment-browser.prod-develop.ts"
                },
                {
                  "replace": "src/manifest.json",
                  "with": "src/manifests/manifest.prod-develop.json"
                },
                {
                  "replace": "src/robots.txt",
                  "with": "src/robots/robots.prod-develop.txt"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ],
              "serviceWorker": true,
              "baseHref": "/app/",
              "progress": true,
              "extractLicenses": true,
              "verbose": false,
              "index": "src/index/prod-develop/index.html"
            },
            "prod-master": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/browser/environment-browser.prod-master.ts"
                },
                {
                  "replace": "src/manifest.json",
                  "with": "src/manifests/manifest.prod-master.json"
                },
                {
                  "replace": "src/robots.txt",
                  "with": "src/robots/robots.prod-master.txt"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ],
              "serviceWorker": true,
              "baseHref": "/app/",
              "progress": true,
              "extractLicenses": true,
              "verbose": false,
              "index": "src/index/prod-master/index.html"
            }
          }
        },
        "serve": {
          "builder": "@angular-builders/custom-webpack:dev-server",
          "options": {
            "customWebpackConfig": {
              "path": "./scripts/webpack/custom-webpack-server.config.js",
              "replaceDuplicatePlugins": false
            },
            "browserTarget": "customer-portal:build",
            "port": 4202,
            "ssl": true,
            "sslKey": "ssl/customer-portal.key",
            "sslCert": "ssl/customer-portal.crt",
            "host": "customer-portal",
            "proxyConfig": "proxy.conf.json"
          },
          "configurations": {
            "dev": {
              "hmr": true,
              "browserTarget": "customer-portal:build:dev",
              "hmrWarning": false
            }
          }
        },
        "test": {
          "builder": "@angular-builders/custom-webpack:karma",
          "options": {
            "customWebpackConfig": {
              "path": "./scripts/webpack/custom-webpack-test.config.js",
              "replaceDuplicatePlugins": false
            },
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "codeCoverage": true,
            "styles": [
              "src/assets/styles/styles.scss"
            ],
            "assets": [
              "src/assets",
              "src/manifest.json",
              "src/robots.txt",
              "src/favicon.ico"
            ],
            "watch": true
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ],
            "typeCheck": true,
            "force": true,
            "format": "codeFrame"
          }
        },
        "server": {
          "builder": "@angular-builders/custom-webpack:server",
          "options": {
            "customWebpackConfig": {
              "path": "./scripts/webpack/custom-webpack-server.config.js",
              "replaceDuplicatePlugins": false
            },
            "outputPath": "dist/server",
            "main": "src/main.server.ts",
            "tsConfig": "src/tsconfig.server.json"
          },
          "configurations": {
            "prod-develop": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/server/environment-server.prod-develop.ts"
                },
                {
                  "replace": "src/manifest.json",
                  "with": "src/manifests/manifest.prod-develop.json"
                },
                {
                  "replace": "src/robots.txt",
                  "with": "src/robots/robots.prod-develop.txt"
                }
              ],
              "bundleDependencies": "all",
              "sourceMap": false,
              "optimization": {
                "scripts": false,
                "styles": true
              }
            },
            "prod-master": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/server/environment-server.prod-master.ts"
                },
                {
                  "replace": "src/manifest.json",
                  "with": "src/manifests/manifest.prod-master.json"
                },
                {
                  "replace": "src/robots.txt",
                  "with": "src/robots/robots.prod-master.txt"
                }
              ],
              "bundleDependencies": "all",
              "sourceMap": false,
              "optimization": {
                "scripts": false,
                "styles": true
              }
            }
          }
        }
      }
    }
  },
  "defaultProject": "wine"
}
